<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="XtInterfaceDao">
	 
	 <resultMap type="XtInterface" id="XtInterface">
			<result property="id" column="Id"/>
			<result property="xtname" column="xtname"/>
			<result property="functionname" column="functionname"/>
			<result property="facetype" column="facetype"/>
			<result property="facename" column="facename"/>
			<result property="remark" column="remark"/>
			<result property="disabled" column="disabled"/>
 	</resultMap>

	<select id="getAllList" resultMap="XtInterface" parameterType="map">
		select `Id` ,
  `xtname` ,
  `functionname`,
  `facetype` ,
  `facename` ,
  `remark` ,
  `disabled`  from XtInterface
  <where>
  
  	 <if test="functionname!=null"> functionname like #{functionname}</if>
  </where>
	</select>
	 
</mapper>