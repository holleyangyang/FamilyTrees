<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="FamilyUser">
	 
	 <resultMap type="FamilyUser" id="familyuser">
			<result property="id" column="id"/>
			<result property="name" column="name"/>
			<result property="treeId" column="treeId"/>
			<result property="sex" column="sex"/>
	</resultMap>
<select id="getUserByTreeId" resultMap="familyuser" parameterType="map">
		select u.id, u.name, u.sex, u.treeid
		from familyuser  u	  <where> treeid=#{treeId} </where>
	</select>
	
	
	 
</mapper>