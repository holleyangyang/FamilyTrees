<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.function.buzhou.dao.BuZhouDao">
	 
	 <resultMap type="map" id="buzhou">
			<result property="id" column="id"/>
			<result property="name" column="name"/>
			<result property="remark" column="remark"/>
	</resultMap>
	<select id="getList" resultMap="buzhou" parameterType="map">
		select u.id, u.name, u.remark
		from SYS_BUZHOU  u
		<where>	 
		 <if test="functionId!=''">
		 	function_id=${functionId}
		 </if>
		 </where>
	</select>
	 
</mapper>